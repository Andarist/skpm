{
  "name": "skpm",
  "version": "1.0.14",
  "description": "A utility to build, publish and install sketch plugins",
  "preferGlobal": true,
  "main": "lib/index.js",
  "bin": {
    "skpm": "lib/index.js"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "lib",
    "check.js"
  ],
  "scripts": {
    "prebuild": "../../node_modules/.bin/rimraf lib",
    "build": "../../node_modules/.bin/babel src -d lib -D",
    "watch": "npm run build -- --watch",
    "prepare": "npm run -s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skpm/skpm.git"
  },
  "keywords": [
    "sketch",
    "cocoascript",
    "javascript",
    "babel",
    "skpm"
  ],
  "author": "Mathieu Dutour <mathieu@dutour.me> (http://mathieu.dutour.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skpm/skpm/issues"
  },
  "homepage": "https://github.com/skpm/skpm#readme",
  "dependencies": {
    "@skpm/internal-utils": "^0.1.8",
    "bluebird": "^3.5.1",
    "chalk": "^1.1.3",
    "cross-spawn-promise": "^0.10.1",
    "fs.promised": "^3.0.0",
    "gittar": "^0.1.0",
    "globby": "^7.1.1",
    "inquirer": "^3.3.0",
    "keytar": "^4.0.4",
    "log-symbols": "^2.1.0",
    "open": "^0.0.5",
    "ora": "^1.3.0",
    "parse-author": "^2.0.0",
    "request": "^2.81.0",
    "update-notifier": "^2.3.0",
    "which": "^1.3.0",
    "xml2js": "^0.4.17",
    "yargs": "^9.0.1"
  }
}
